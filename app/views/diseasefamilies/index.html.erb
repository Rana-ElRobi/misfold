<p id="notice"><%= notice %></p>

<h1>Listing Diseasefamilies</h1>

<table>
  <thead>
    <tr>
      <th>Familyid</th>
      <th>Familyname</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diseasefamilies.each do |diseasefamily| %>
      <tr>
        <td><%= diseasefamily.familyid %></td>
        <td><%= diseasefamily.familyname %></td>
        <td><%= link_to 'Show', diseasefamily %></td>
        <td><%= link_to 'Edit', edit_diseasefamily_path(diseasefamily) %></td>
        <td><%= link_to 'Destroy', diseasefamily, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diseasefamily', new_diseasefamily_path %>
