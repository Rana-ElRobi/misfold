<p id="notice"><%= notice %></p>

<h1>Listing Diseasesubsets</h1>

<table>
  <thead>
    <tr>
      <th>Subset</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diseasesubsets.each do |diseasesubset| %>
      <tr>
        <td><%= diseasesubset.subset %></td>
        <td><%= link_to 'Show', diseasesubset %></td>
        <td><%= link_to 'Edit', edit_diseasesubset_path(diseasesubset) %></td>
        <td><%= link_to 'Destroy', diseasesubset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diseasesubset', new_diseasesubset_path %>
