<p id="notice"><%= notice %></p>

<h1>Listing Omims</h1>

<table>
  <thead>
    <tr>
      <th>Omimid</th>
      <th>Omimurl</th>
      <th>Moodofinheritnce</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @omims.each do |omim| %>
      <tr>
        <td><%= omim.omimid %></td>
        <td><%= omim.omimurl %></td>
        <td><%= omim.moodofinheritnce %></td>
        <td><%= link_to 'Show', omim %></td>
        <td><%= link_to 'Edit', edit_omim_path(omim) %></td>
        <td><%= link_to 'Destroy', omim, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Omim', new_omim_path %>
