<p id="notice"><%= notice %></p>

<h1>Listing Proteinontologies</h1>

<table>
  <thead>
    <tr>
      <th>Prid</th>
      <th>Name</th>
      <th>Definition</th>
      <th>Comment</th>
      <th>Synonym</th>
      <th>Namespace</th>
      <th>Xref</th>
      <th>Is obsolete</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proteinontologies.each do |proteinontology| %>
      <tr>
        <td><%= proteinontology.prid %></td>
        <td><%= proteinontology.name %></td>
        <td><%= proteinontology.definition %></td>
        <td><%= proteinontology.comment %></td>
        <td><%= proteinontology.synonym %></td>
        <td><%= proteinontology.namespace %></td>
        <td><%= proteinontology.xref %></td>
        <td><%= proteinontology.is_obsolete %></td>
        <td><%= link_to 'Show', proteinontology %></td>
        <td><%= link_to 'Edit', edit_proteinontology_path(proteinontology) %></td>
        <td><%= link_to 'Destroy', proteinontology, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proteinontology', new_proteinontology_path %>
