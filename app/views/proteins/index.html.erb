<p id="notice"><%= notice %></p>

<h1>Listing Proteins</h1>

<table>
  <thead>
    <tr>
      <th>Uniprot</th>
      <th>Name</th>
      <th>Length</th>
      <th>Sequence</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proteins.each do |protein| %>
      <tr>
        <td><%= protein.uniprot_id %></td>
        <td><%= protein.name %></td>
        <td><%= protein.length %></td>
        <td><%= protein.sequence %></td>
        <td><%= link_to 'Show', protein %></td>
        <td><%= link_to 'Edit', edit_protein_path(protein) %></td>
        <td><%= link_to 'Destroy', protein, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Protein', new_protein_path %>
